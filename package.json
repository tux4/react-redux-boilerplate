{
  "name": "react-redux-boilerplate",
  "version": "1.0.0",
  "description": "My react redux boilerplate",
  "main": "src/index.js",
  "scripts": {
    "start": "node ./dev_config/webpack_server.js",
    "test": "./node_modules/.bin/mocha --recursive --compilers js:babel-register",
    "test:watch": "npm test -- --watch",
    "build": "NODE_ENV=production ./node_modules/.bin/webpack --config dev_config/webpack.config.build.js",
    "build:var": "NODE_ENV=production ./node_modules/.bin/webpack --config dev_config/webpack.config.var.js",
    "build:commonjs": "NODE_ENV=production ./node_modules/.bin/webpack --config dev_config/webpack.config.common.js"
  },
  "author": "Prasanna Suman",
  "license": "GPL-3.0",
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-class-properties": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.4.3",
    "expect": "^1.14.0",
    "jsdom": "^7.2.2",
    "mocha": "^2.4.5",
    "mocha-jsdom": "^1.0.0",
    "react-addons-test-utils": "^0.14.7",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^3.0.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.2",
    "redux-thunk": "^1.0.3",
    "sinon": "^1.17.3",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "history": "^1.17.0",
    "react": "^0.14.5",
    "react-dom": "^0.14.6",
    "react-redux": "^4.0.6",
    "redux": "^3.0.5",
    "redux-simple-router": "^1.0.2"
  }
}
